{
  "name": "dustinruetz.com",
  "version": "0.0.0-semantically-released",
  "description": "Dustin Ruetz's website.",
  "scripts": {
    "build": "webpack --config ./config/webpack.prod.js",
    "build:local": "rm -rf ./www/ && webpack --config ./config/webpack.prod.js",
    "dev:local": "npm run nodemon -- --exec 'webpack-dev-server --config ./config/webpack.dev-local.js'",
    "dev:network": "npm run nodemon -- --exec 'webpack-dev-server --config ./config/webpack.dev-network.js'",
    "format": "dr-scripts format",
    "lint": "dr-scripts lint",
    "nodemon": "nodemon --config ./config/nodemon.json",
    "test": "dr-scripts test",
    "test:coverage": "dr-scripts test --coverage"
  },
  "husky": {
    "hooks": {
      "commit-msg": "dr-scripts commit-msg",
      "pre-commit": "dr-scripts pre-commit"
    }
  },
  "homepage": "https://github.com/dustin-ruetz/dustinruetz.com/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dustin-ruetz/dustinruetz.com.git"
  },
  "author": "Dustin Ruetz",
  "license": "MIT",
  "engines": {
    "node": ">= 12",
    "npm": ">= 6"
  },
  "dependencies": {},
  "devDependencies": {
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.3.0",
    "dr-scripts": "^2.2.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "image-webpack-loader": "^7.0.1",
    "mini-css-extract-plugin": "^0.11.2",
    "nodemon": "^2.0.4",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "eslintConfig": {
    "extends": "./node_modules/dr-scripts/src/config/.eslintrc.js"
  },
  "prettier": "./node_modules/dr-scripts/src/config/.prettierrc.js"
}
